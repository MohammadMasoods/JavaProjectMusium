import java.util.ArrayList;
import java.util.Date;


public class Visitor extends person{
	int record_id;
	int Num_ofVisit;
	
	public Visitor(String Name, String address, int ID, String NOT, Date dob,int record_id,int Num_ofVisit) {
		super(Name, address, ID, NOT, dob);
		setRecord_id(record_id);
		setNum_ofVisit(Num_ofVisit);
		
	}


	
	
	
	public int getRecord_id() {
		return record_id;
	}
	public void setRecord_id(int record_id) {
		this.record_id = record_id;
	}
	public int getNum_ofVisit() {
		return Num_ofVisit;
	}
	public void setNum_ofVisit(int num_ofVisit) {
		Num_ofVisit = num_ofVisit;
	}
	public void book_tour() {
		
		
		
		
		
	}
	 
	public int getV_id() {
		return this.getV_id();
		
		
	}
	public void SetV_id(int V_id) {
		this.SetV_id(V_id);
	}
	ArrayList <VisitorRecord> getVisits(){
		
		return this.getVisits();

	}
	 public void set_Visits (ArrayList <VisitorRecord> Vrecord) {
		
		this.set_Visits(Vrecord);
	}
	
	
}