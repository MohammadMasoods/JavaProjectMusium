
public enum WorkingDay {
	
	
	SUNDAY(1) , MONDAY(2),TUSEDAY(3)  ,WEDNESDAY(4), THURSDAY(5);

	   private final int hierarchy;

	    private WorkingDay(int hierarchy) {
	        this.hierarchy = hierarchy;
	    }

	    public int getHierarchy() {
	        return hierarchy;
	    }

