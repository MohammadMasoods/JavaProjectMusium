import java.util.*;


public class Employee extends person{
	 Employee(String Name, String address, int ID, String NOT, Calendar dob,Calendar hire ,int employee_id) {
		super(Name, address, ID, NOT, dob);
		this.employee_id=employee_id;
		this.hire_Date=hire;
	}
	 Employee (){};
	Calendar hire_Date;
	 Random employee_id;
	
	
	public Calendar getHire_Date() {
		return hire_Date;
	}
	public void setHire_Date(Calendar hire_Date) {
		this.hire_Date = hire_Date;
	}
	public Random getEmployee_id() {
		return employee_id;
	}
	public void setEmployee_id(Random r) {
		this.employee_id = r;
	}
	@Override
	public String toString() {
		return null;
	}
	 
	
}